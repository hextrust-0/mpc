// -*- go -*-

package main

import (
	"crypto/hmac"
)

// HMAC-SHA256 tests:
//
// "abc", "message": 859cc656e12c0ecd0afdd7e3d034c3ee81609fcac1b454c231211c7ac69895e8

func main(g, e byte) []byte {
	var key [3]byte
	key[0] = 'a'
	key[1] = 'b'
	key[2] = 'c'

	var msg [7]byte
	msg[0] = 'm'
	msg[1] = 'e'
	msg[2] = 's'
	msg[3] = 's'
	msg[4] = 'a'
	msg[5] = 'g'
	msg[6] = 'e'

	return hmac.SumSHA256(msg[:], key[:])
}
