// -*- go -*-

package main

type FieldElement [10]int32

var zero FieldElement

func FeZero(fe *FieldElement) {
	copy(fe[:], zero[:])
}

func FeOne(fe *FieldElement) {
	FeZero(fe)
	fe[0] = 1
}

// @Test 0 0 = 1
// @Test 1 2 = 4
func main(a, b int32) int {
	var v FieldElement

	FeOne(&v)

	return a + b + v[0]
}
